<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SettingLayout">
        <attr name="setting_title" format="string"/>
        <attr name="setting_sub_title" format="string"/>
    </declare-styleable>
    <declare-styleable name="Lrc">
        <attr name="lrctextSize" format="dimension" />
        <attr name="normalTextColor" format="reference|color" />
        <attr name="currentTextColor" format="reference|color" />
        <attr name="dividerHeight" format="dimension" />
        <attr name="rows" format="integer" />
    </declare-styleable>
    <declare-styleable name="playerview">
        <attr name="cover" format="integer" />
        <attr name="buttonColor" format="color"/>
        <attr name="progressEmptyColor" format="color"/>
        <attr name="progressLoadedColor" format="color"/>
        <attr name="textColor" format="color"/>
        <attr name="textSize" format="dimension"/>
    </declare-styleable>
</resources>