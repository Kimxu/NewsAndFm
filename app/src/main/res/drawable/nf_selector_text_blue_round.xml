<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@android:color/white" />
            <stroke android:width="0.5dp" android:color="@color/nf_text_disable" />
            <corners android:radius="10dp" />
            <size android:width="50dp" android:height="20dp" />
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/nf_text_blue_normal" />
            <corners android:radius="10dp" />
            <size android:width="50dp" android:height="20dp" />
        </shape>
    </item>

    <item>
        <shape>
            <solid android:color="@color/nf_white" />
            <corners android:radius="10dp" />
            <stroke android:width="0.5dp" android:color="@color/nf_text_blue_normal" />
            <size android:width="50dp" android:height="20dp" />
        </shape>
    </item>
</selector>